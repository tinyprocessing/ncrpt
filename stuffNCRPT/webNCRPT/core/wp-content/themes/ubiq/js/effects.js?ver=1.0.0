var rtime;
var timeout = false;
var delta = 200;
$(window).resize(function() {
    rtime = new Date();
    if (timeout === false) {
        timeout = true;
        setTimeout(resizeend, delta);
    }
});

function resizeend() {
    if (new Date() - rtime < delta) {
        setTimeout(resizeend, delta);
    } else {
        timeout = false;
         multislider();
         $("#l_0").click();
    }               
}

function multislider(){
	var lefT_i = numItems = lefT_w = 0;
	$(".custom_slider .left_inner,.custom_slider .left_text,.custom_slider .top_images_cont, .custom_slider .bottom_inner,.custom_slider .img_cont_sli, .custom_slider .icon_set").css( "width", "auto");
	$(".left_inner,.top_images_cont,.bottom_inner").css( "left", "0" );
	$(".top_slider_button").removeClass("active");
	$("#q_0").addClass("active");    		


	numItems = $('.logo_cont').length
    lefT_w = $(".left_side").width();
    $(".custom_slider .left_inner").css( "width", lefT_w*numItems );
    $(".custom_slider .left_text").css( "width", lefT_w );

    lefT_i = $(".top_images").width();
    $(".custom_slider .top_images_cont, .custom_slider .bottom_inner").css( "width", lefT_i*numItems );
    $(".custom_slider .img_cont_sli, .custom_slider .icon_set").css( "width", lefT_i );




    $(".logo_cont").click(function(){
    	var ids = $(this).attr("ids");
    	lefT_i = $(".top_images").width();
    	lefT_w = $(".left_side").width();
    	$(".top_images_cont").animate({
            left: "-"+lefT_i*ids
        }, 300);

    	setTimeout(function() {
	    	$(".left_inner").animate({
	            left: "-"+lefT_w*ids
	        }, 300);
    	}, 200);

    	$(".logo_cont").removeClass("active");
    	$("#l_"+ids).addClass("active");
    });

    $(".top_slider_button").click(function(){
    	if($(this).hasClass("active")){

    	} else {
    		lefT_i = $(".top_images").width();
	    	var idq = $(this).attr("idq");
	    	$(".bottom_inner").animate({
	            left: "-"+lefT_i*idq
	        }, 300);

	    	$(".top_slider_button").removeClass("active");
	    	$("#s_"+idq+" div").first().click();
	    	$(this).addClass("active");    		

    	}

    });
}