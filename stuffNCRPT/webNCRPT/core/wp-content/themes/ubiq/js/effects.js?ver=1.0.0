var $ = jQuery;

$(document).scroll(function() {
    var top = $(document).scrollTop();
    if(top > 1){
        $("#masthead").addClass("scrolled");
    } else {
        $("#masthead").removeClass("scrolled");
    }
});



jQuery(document).ready(function($) {

	$(document).on('click', 'a[href^="#"]', function (event) {
	    event.preventDefault();

	    $('html, body').animate({
	        scrollTop: $($.attr(this, 'href')).offset().top-120
	    }, 500);
	});


	if ($(window).width() < 1040) {
		$(".main-navigation ul li.menu-item-has-children").click(function(){
		 	if($(this).hasClass("hovering")){
	        	$(this).removeClass("hovering").find(".sub-menu").slideUp(200);        
		 	} else {
	        	$(this).addClass("hovering").find(".sub-menu").slideDown(200);
		 	}
	    });
	} else {
	    $(".main-navigation ul li.menu-item-has-children").mouseenter(function(){
	        $(this).addClass("hovering").find(".sub-menu").stop().fadeIn(200);
	    }).mouseleave(function(event) {
	        $(this).removeClass("hovering").find(".sub-menu").stop().fadeOut(200);        
	    });		
	}



    $(".tabs .tabs_top .tab_o").click(function(){
        var ids = $(this).attr("ids");
        if($(this).hasClass("active")){

        } else {
            $(".tab_o,.tab_content").removeClass("active");
            $(this).addClass("active");
            $("#tab_"+ids).addClass("active");


            $(".tabs .tab_content.active").addClass("done");

       		$(".tabs .tab_content.active").find('.item').each( function(k, v) {
		    var el = this;
		        setTimeout(function () {
		        $(el).addClass("shown");
		    }, k*400);
		        //Want to add "taskshown" class to the DIVs one after another
		    })
        }
    });

    $(".left_side_code .left_tabs .tab_item").click(function(){
        var ids = $(this).attr("ids");
        if($(this).hasClass("active")){

        } else {
            $(".tab_item,.code_list").removeClass("active");
            $(this).addClass("active");
            $("#code_x"+ids).addClass("active");
        }
    });


    //slider
    multislider();

    

    $(".open_positions .position .left_side,.open_positions .position .right_side").click(function(){
    	if($(this).parent().hasClass("active")){
    		$(this).parent().removeClass("active");
    		$(this).parent().find(".form").slideUp();
    	} else {
    		$(this).parent().addClass("active");
    		$(this).parent().find(".form").slideDown();
    	}
    });


    $(".faq .faq_items .item").click(function(){
    	if($(this).hasClass("active")){
    		$(this).removeClass("active");
    		$(this).find(".text").slideUp();
    	} else {
    		$(this).addClass("active");
    		$(this).find(".text").slideDown();
    	}
    });

    $(".faq .show_more").click(function(event) {
    	if($(this).hasClass("active")){
    		$(this).removeClass("active").html("More FAQs");
    		$(".faq .faq_items .item.hidden").slideUp();
    	} else {
    		$(this).addClass("active").html("Less FAQs");
    		$(".faq .faq_items .item.hidden").slideDown();
    	}
		
    });

    $(".faq .tags_top .tag_top").click(function(event) {
    	var idq = $(this).attr("ids");
    	console.log(idq);
    	if(idq == "all"){
    		$(".faq .faq_items .item").slideDown();
    		$(".faq .tags_top .tag_top").removeClass("active");
	    		$(this).addClass("active");
    	} else {    		
	    	if($(this).hasClass("active")){
	    	} else {
	    		$(".faq .tags_top .tag_top").removeClass("active");
	    		$(this).addClass("active");
	    		$(".faq .faq_items .item").slideUp();
	    		$(".faq .faq_items .item."+idq).slideDown();
	    	}
    	}

		
    });

    $(".pricing .view_more_plans").click(function(event) {
    	if($(this).hasClass("active")){
    		$(this).removeClass("active").html("View More");
    		$(".pricing .sl.hidden").slideUp();
    	} else {
    		$(this).addClass("active").html("View Less");
    		$(".pricing .sl.hidden").slideDown();
    	}
		
    });

    $(".load_more_posts").click(function(){

    	var nr = $(".load_more_posts").attr("pg");
        var total_items = $(".load_more_posts").attr("pt");
        var per_page_items = $(".load_more_posts").attr("pr");
        var sk = $(".load_more_posts").attr("sk");

        var new_nr = Number(nr)+Number(1);

        if(new_nr < (total_items/per_page_items)){ 
            $(".show_more").show();
            $(".load_more_posts").attr("pg", new_nr);
            load_more_posts(new_nr, sk);
        } else {
            $(".show_more").hide();
        }
	});


	$(".filter.tags .sg_tag").click(function(){

		if($(this).hasClass("active")){
			$(this).removeClass("active");
			$(".postitems").empty();
    		$(".load_more_posts").attr("pg", 1);
		} else {
			$(".filter.tags .sg_tag").removeClass("active");
			$(this).addClass("active");
		}

		load_more_posts(0);
	});

	$(".search_bar").on('keyup', function (e) {
	    if (e.key === 'Enter' || e.keyCode === 13) {
	       load_more_posts(0);
	    }
	});

	$(".open_menu").click(function(){
		$("#site-navigation").show();
	});

	$(".close_menu").click(function(){
		$("#site-navigation").hide();
	});


	if ($(".tabs")[0]){
		var scrollTop     = $(window).scrollTop(),
	    elementOffset = $('.tabs').offset().top,
	    distance      = (elementOffset - scrollTop);
	}

	if ($(".how_to_get_started")[0]){
		var scrollTop_started     = $(window).scrollTop(),
	    elementOffset_started = $('.how_to_get_started').offset().top,
	    distance_started      = (elementOffset_started - scrollTop_started);
	}

   	$( window ).scroll(function(){
       var scrolled = $(window).scrollTop() ;
       if ($(".tabs")[0]){
	       if((scrolled+500) > distance){
	       		
	       		if($(".tabs .tab_content.active").hasClass("done")){}else{
		       		$(".tabs .tab_content.active").addClass("done");

		       		$(".tabs .tab_content.active").find('.item').each( function(k, v) {
				    var el = this;
				        setTimeout(function () {
				        $(el).addClass("shown");
				    }, k*400);
				        //Want to add "taskshown" class to the DIVs one after another
				    })

		       	}
	       }
       }

       if ($(".how_to_get_started")[0]){
	       if((scrolled+200) > distance_started){
	       		
	       		if($(".how_to_get_started").hasClass("done")){}else{
		       		$(".how_to_get_started").addClass("done");

		       		$(".how_to_get_started .repeater_items").find('.item').each( function(k, v) {
				    var el = this;
				        setTimeout(function () {
				        $(el).addClass("shown");
				    }, k*400);
				        //Want to add "taskshown" class to the DIVs one after another
				    })

		       	}
	       }
       }
    });


   	if ($(".in_view_top")[0]){
	    LottieInteractivity.create({
		    player:'.in_view_top',
		    mode:"scroll",
		    actions: [
		        {
		        visibility: [0, 1.0],
		        type: "play"
		        }
		    ]
		});
	}

   	if ($(".in_view")[0]){
	    LottieInteractivity.create({
		    player:'.in_view',
		    mode:"scroll",
		    actions: [
		        {
		        visibility: [0.3, 1.0],
		        type: "play"
		        }
		    ]
		});
	}

	if ($(".in_view_second")[0]){

		LottieInteractivity.create({
		    player:'.in_view_second',
		    mode:"scroll",
		    actions: [
		        {
		        visibility: [0.3, 1.0],
		        type: "play"
		        }
		    ]
		});
	}

});







function load_more_posts(nr, sk){

    var total_items = $(".load_more_posts").attr("pt");
    var per_page_items = $(".load_more_posts").attr("pr");
    if(!nr){
        var nr = $(".load_more_posts").attr("pg");
    }    

    var search = $(".search_bar").val();
    var cats = $(".filter.tags .sg_tag.active").attr("ids");

    if(cats || search){
    	$(".blog_page .left_side .blog_post.featured").hide();
    	$(".postitems").empty();
    	$(".load_more_posts").attr("pg", 0);
    	var nr = 0;
    } else {
    	$(".blog_page .left_side .blog_post.featured").show();    	
    }

    var data = {
        action      : 'load_new_posts',
        search      : search,
        page_nr     : nr,
        skip        : sk,
        per_page    : per_page_items,
        total       : total_items,
        cats        : cats
    }

    jQuery.ajax({
        type : "post",
        dataType : "json",
        url : myAjax.ajaxurl,
        data : data,
        success: function(response) {
            $(".postitems").append(response.items);
            //$('html, body').animate({ scrollTop: $(".load_more_posts").offset().top}, 500);            
        }
    });    
}

